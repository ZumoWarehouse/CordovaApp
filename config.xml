<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.zumowarehouse" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Zumo Warehouse</name>
    <description>
    </description>
    <author email="boyko.ant@live.com">
        Boyko Anton
    </author>
    <content src="app.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-ms-azure-mobile-apps" spec="~2.0.0-rc1" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="android" spec="~6.0.0" />
    <engine name="ios" spec="~4.3.0" />
</widget>
